{
  "manifest_version": 3,
  "name": "Chrome WebAI Extension",
  "description": "Extension to help developers with their Built-In AI APIs.",
  "version": "0.2.1",
  "action": {
    "default_popup": "",
    "default_icon": "assets/images/icon.png"
  },
  "icons": {
    "128": "assets/images/icon.png"
  },
  "permissions": [],
  "devtools_page": "devtools-loader.html",
  "background": {
    "service_worker": "service_worker.js",
    "type": "module"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "assets/images/spark.png"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}